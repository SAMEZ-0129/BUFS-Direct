{
    "manifest_version": 3,	
    "name": "Quick BUFS",
    "version": "1.0.1",		
    "description": "부산외국어대학교 재학생들이 많이 사용하는 사이트 모음집!", 
    "icons": {
      "16": "./icons/16logo.png",
      "48": "./icons/48logo.png",
      "128": "./icons/128logo.png"	
    },
    "action": {
      "default_popup": "./popup/index.html",
      "default_icon": {
        "16": "./icons/16logo.png"	
      }
    },
    "permissions": [
      "https://*www.bufs.ac.kr/*",
      "https://m.bufs.ac.kr/default.aspx?ReturnUrl=%2f",
      "https://eclass.bufs.ac.kr/",
      "https://sugang.bufs.ac.kr/Login.aspx",
      "https://github.com/*"
    ]
  }
  